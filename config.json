{
    "academic_terms": [
        {"name": "id", "module": "None"},
        {"name": "canvas_id", "module": "None"},
        {"name": "name", "module": "None"},
        {"name": "date_start", "module": "None"},
        {"name": "date_end", "module": "None"}
    ],
    "course": [
        {"name": "id", "module": "ffx", "method": "encrypt"},
        {"name": "canvas_id", "module": "ffx", "method": "encrypt"},
        {"name": "term_id", "module": "None"},
        {"name": "name", "module": "faker", "method": "course"}
    ],
    "resource_access": [
        {"name": "id", "module": "None"},
        {"name": "resource_id", "module": "ffx", "method": "encrypt"},
        {"name": "user_id", "module": "ffx", "method": "encrypt"},
        {"name": "access_time", "module": "shuffle"}
    ],
    "resource": [
        {"name": "id", "module": "ffx", "method": "encrypt"},
        {"name": "resource_type", "module": "shuffle", "index": "course_id"},
        {"name": "name", "module": "faker", "method": "file_name"},
        {"name": "course_id", "module": "ffx", "method": "encrypt"}
    ],
    "user": [
        {"name": "id", "module": "None"},
        {"name": "user_id", "module": "ffx", "method": "encrypt"},
        {"name": "name", "module": "faker", "method": "name"},
        {"name": "sis_id", "module": "ffx", "method": "encrypt"},
        {"name": "sis_name", "module": "faker", "method": "user_name"},
        {"name": "course_id", "module": "ffx", "method": "encrypt"},
        {"name": "current_grade", "module": "redist", "index": "course_id"},
        {"name": "final_grade", "module": "redist", "index": "course_id"}
    ],
    "assignment_groups": [
        {"name": "id", "module": "ffx", "method": "encrypt"},
        {"name": "name", "module": "faker", "method": "assignment"},
        {"name": "weight", "module": "redist", "index": "course_id"},
        {"name": "group_points", "module": "redist", "index": "course_id"}, 
        {"name": "course_id", "module": "ffx", "method": "encrypt"},
        {"name": "drop_lowest", "module": "None"},
        {"name": "drop_highest", "module": "None"}
    ],
    "assignment@id|submission@id" : {
        "join" : "LEFT JOIN submission on (assignment.id = submission.assignment_id)",
        "tables" : [
            {
                "name": "assignment",
                "cols": [
                    {"name": "id", "module": "ffx", "method": "encrypt"},
                    {"name": "name", "module": "faker", "method": "assignment"},
                    {"name": "due_date", "module": "faker", "method": "date_time_on_date"},
                    {"name": "local_date", "module": "faker", "method": "date_time_on_date"},
                    {"name": "points_possible", "module": "redist", "index": "assignment.course_id"},
                    {"name": "course_id", "module": "ffx", "method": "encrypt"},
                    {"name": "assignment_group_id", "module": "ffx", "method": "encrypt"}
                ]
            },
            {
                "name": "submission",
                "cols": [
                    {"name": "id", "module": "ffx", "method": "encrypt"},
                    {"name": "assignment_id", "module": "ffx", "method": "encrypt"},
                    {"name": "course_id", "module": "ffx", "method": "encrypt"},
                    {"name": "user_id", "module": "ffx", "method": "encrypt"},
                    {"name": "score", "module": "ffx", "method": "encrypt"},
                    {"name": "graded_date", "module": "faker", "method": "date_time_on_date"},
                    {"name": "avg_score", "module": "mean", "source": "submission.score", "index": "submission.assignment_id"}
                ]
            }
        ]
    },
    "assignment_weight_consideration": [
        {"name": "course_id", "module": "ffx", "method": "encrypt"},
        {"name": "consider_weight", "module": "None"}
    ],
    "course_view_option": [
        {"name": "course_id", "module": "ffx", "method": "encrypt"},
        {"name": "show_files_accessed", "module": "None"},
        {"name": "show_assignment_planning", "module": "None"},
        {"name": "show_grade_distribution", "module": "None"}
    ],
    "user_default_selection": [
        {"name": "id", "module": "ffx", "method": "encrypt"},
        {"name": "course_id", "module": "ffx", "method": "encrypt"},
        {"name": "user_sis_name", "module": "ffx", "method": "encrypt"},
        {"name": "default_view_type", "module": "None"},
        {"name": "default_view_value", "module": "None"}
    ]
}